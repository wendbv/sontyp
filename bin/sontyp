#!/usr/bin/env node
if(process.argv[2]) {
    var fs = require("fs");
    var file = fs.readFileSync(process.argv[2], {encoding: "utf-8"});

    var obj = JSON.parse(file);

    var sontyp = require('./../lib/sontyp.js');
    sontyp(obj);
} else {
    console.log('Usage: sontyp <url-or-path>\n');
}
